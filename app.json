{
  "expo": {
    "name": "auto-harvest-ui",
    "slug": "auto-harvest-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Auto-Harvest-noBG.png",
    "scheme": "auto-harvest-ui",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.CHANGE_WIFI_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Auto-Harvest-noBG.png",
        "backgroundColor": "#d6d6c4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Auto-Harvest-noBG.png"
    },
    "plugins": [
      "expo-router",
      "react-native-wifi-reborn",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Auto-Harvest-noBG.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#d6d6c4"
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
